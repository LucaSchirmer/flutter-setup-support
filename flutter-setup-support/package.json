{
  "name": "flutter-setup-support",
  "publisher": "LucaSchi",
  "displayName": "flutter setup support",
  "description": "A VSCode Extension to support an easier setup for app-development using flutter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repo.git"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "flutter-setup-support.createAppNavigationHandler",
      "title": "Create App Navigation Handler"
    },{
      "command": "flutter-setup-support.createNavigationSetUp",
      "title": "Create dynamic Navigationbar set up"
    }
  
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
